{
  "name": "laurynasgadl/php-validator",
  "type": "library",
  "description": "Array value validation library for PHP",
  "keywords": ["php","array","validation","validator"],
  "license": "MIT",
  "authors": [
      {
          "name": "Laurynas Gadliauskas",
          "homepage": "https://github.com/laurynasgadl"
      }
  ],
  "require": {
    "php": ">=5.6",
    "laurynasgadl/travers": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Luur\\Validator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Luur\\Validator\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
